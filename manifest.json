{
    "name": "YAAMUTE",
    "version": "0.1.0",
    "description": "Yet another auto mute extension",
    "manifest_version": 2,
    "icons": {
        "48": "src/images/icons/icon-48.png",
        "96": "src/images/icons/icon-96.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "yaamute@fazekmar",
            "strict_min_version": "68.0"
        }
    },
    "permissions": [
        "storage",
        "menus",
        "tabs"
    ],
    "options_ui": {
        "page": "src/options/options.html"
    },
    "background": {
        "scripts": [
            "src/background.js"
        ]
    }
}
